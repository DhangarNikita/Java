
public class Fibonacci {

	static int factorial(int n) {
		if(n==0|| n==1) {
			System.out.println(n);
			return n;
		}
		return factorial(n-1)+factorial(n-2);
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0;i<=10;i++) {
			System.out.println(factorial(i));
		}

	}

}