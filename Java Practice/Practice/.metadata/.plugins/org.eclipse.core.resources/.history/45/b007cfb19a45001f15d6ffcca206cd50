
public class Encapsulation {

	
		private int empid;
		
		public void setEmpid(int eid) {
			empid=eid;
		}
		public int getEmpid() {
			return empid;
		}
	}


class Employee extends Encapsulation{
	public static void main(String[]args) {
		Employee em=new Employee();
		em.setEmpid(20);
		em.getEmpid();
		
		
	}
	
}