//write a code for sort 0 and 1
package com.TowPointer;
import java.util.Scanner;

class main{
	static void printArray(int []arr) {
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]+" ");
		}
	}
	
	static void sortArray(int arr[]) {
		int count=0;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]==0) {
				count++;
			}
		}
		
		for(int i=0; i<arr.length;i++) {
			if(i<count) {
				arr[i]=0;
			}else {
				arr[i]=1;
			}
		}
		
	}
}
public class Sort {

}
